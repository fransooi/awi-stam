{
    "configurations": [
        {
            "name": "AWI",
            "program": "${workspaceFolder}/engine/simple-node-prompt.mjs",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": ["--preserve-symlinks"],
            "console": "integratedTerminal",
            "outputCapture": "std",
            "type": "node"
        },
        {
            "name": "STAM",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:5173?debug=1",
            "webRoot": "${workspaceFolder}/stam/src",
        }
    ]
}